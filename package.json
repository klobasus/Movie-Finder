{
  "name": "movie-finder",
  "version": "1.0.0",
  "description": "Javascript app for searching movies",
  "main": "index.js",
  "scripts": {
    "start": "parcel src/index.html --no-cache --open",
    "prebuild": "rimraf dist",
    "build": "parcel build src/index.html --public-url ./"
  },
  "keywords": [
    "movie",
    "search",
    "finder"
  ],
  "author": "Lukáš Rác",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "babel-polyfill": "^6.26.0",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-custom-dist-structure": "^1.1.3",
    "parcel-plugin-nuke-dist": "^1.0.1",
    "sass": "^1.26.3"
  },
  "customDistStructure": {
    "config": {
      ".js": "js",
      ".js.map": "js",
      ".css": "css",
      ".css.map": "css",
      "images": [
        ".jpg",
        ".png",
        ".ico",
        ".svg"
      ],
      ".fileExtension": "folder/in/dist",
      "folder/in/dist": [
        ".file",
        ".extensions"
      ]
    }
  }
}
